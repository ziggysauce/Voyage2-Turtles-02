
{
  "name": "DevTab",
  "description": "New tab extension that provides tools for front-end developers",
  "manifest_version": 2,
  "version": "1.1.0",
  "icons": {
    "128": "assets/img/devtab5.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "assets/img/devtab5.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["app.min.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "all_urls"
  ]
}
